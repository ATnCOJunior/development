                    <i class=""></i> <span class="sidebar-nav-mini-hide">The<strong>Foodie Market</strong></span>
                </a>
            </div>
            <!-- END Sidebar Brand -->

            <!-- Wrapper for scrolling functionality -->
            <div id="sidebar-scroll">
                <!-- Sidebar Content -->
                <div class="sidebar-content">
                    <!-- Sidebar Navigation -->
                    <ul class="sidebar-nav">

                        <li class="sidebar-separator">
                            <i class="fa fa-ellipsis-h"></i>
                        </li>

                        <li>
                            <a href="/admin" class="active">
                                <i class="fa fa-eye sidebar-nav-icon"></i>
                                <span class="sidebar-nav-mini-hide">Ads Approval</span>
                            </a>
                        </li>

                        <li>
                            <a href="#" class="sidebar-nav-menu">
                                <i class="fa fa-chevron-left sidebar-nav-indicator sidebar-nav-mini-hide"></i>
                                <i class="fa fa-users sidebar-nav-icon"></i>
                                <span class="sidebar-nav-mini-hide">Customer Management</span>
                            </a>
                            <ul>
                                <li>
                                   <a href="/admin-account-user">Account Table</a>  
                               </li>
                           </ul>
                       </li>
                       <li>
                        <a href="#" class="sidebar-nav-menu">
                            <i class="fa fa-chevron-left sidebar-nav-indicator sidebar-nav-mini-hide"></i>
                            <i class="fa fa-briefcase sidebar-nav-icon"></i>
                            <span class="sidebar-nav-mini-hide">Merchant Management</span>
                        </a>
                        <ul>
                            <li>
                                <a href="/admin-add-merchant">Add a Merchant</a>
                            </li>
                            <li>
                                <a href="/admin-account-merchant">Account Table</a>
                            </li>
                        </ul>
                    </li>
                    
                    <li>
                        <a href="#" class="sidebar-nav-menu">
                            <i class="fa fa-chevron-left sidebar-nav-indicator sidebar-nav-mini-hide"></i>
                            <i class="fa fa-dollar sidebar-nav-icon"></i>
                            <span class="sidebar-nav-mini-hide">Transactions</span>
                        </a>
                        <ul>
                            <li>
                                <a href="admin-transaction-merchant">Merchant Payment</a>
                            </li>
                            <li>
                                <a href="admin-transaction-user">Customer Cashout</a>
                            </li>
                        </ul>
                    </li>

                    <li>
                        <a href="/admin-inbox">
                            <i class="fa fa-inbox sidebar-nav-icon"></i>
                            <span class="sidebar-nav-mini-hide">Inbox</span>
                        </a>
                    </li>
                </ul>
                <!-- END Sidebar Navigation -->
            </div>
            <!-- END Sidebar Content -->
        </div>
        <!-- END Wrapper for scrolling functionality -->

        <!-- Sidebar Extra Info -->
        <div id="sidebar-extra-info" class="sidebar-content sidebar-nav-mini-hide">
            <div class="text-center">
                <small>Crafted with <i class="fa fa-heart text-danger"></i> by AT+CO.JR. </small>
            </div>
        </div>
        <!-- END Sidebar Extra Info -->
    </div>
    <!-- END Main Sidebar -->

    <!-- Main Container -->
    <div id="main-container">

        <header class="navbar navbar-inverse navbar-fixed-top">
            <!-- Left Header Navigation -->
            <ul class="nav navbar-nav-custom">
                <!-- Main Sidebar Toggle Button -->

                <li>
                    <a href="javascript:void(0)" onclick="App.sidebar('toggle-sidebar');">
                        <i class="fa fa-ellipsis-v fa-fw animation-fadeInRight" id="sidebar-toggle-mini" style="margin-bottom: 13px; margin-top: 13px;"></i>
                        <i class="fa fa-bars fa-fw animation-fadeInRight" id="sidebar-toggle-full" style="margin-bottom: 13px; margin-top: 13px;"></i>
                    </a>
                </li>
                <!-- END Main Sidebar Toggle Button -->

                <!-- Header Link -->
                <li class="hidden-xs animation-fadeInQuick">
                    <a><strong>Admin Console</strong></a>
                </li>
                <!-- END Header Link -->
            </ul>
            <!-- END Left Header Navigation -->

            <!-- Right Header Navigation -->
            <ul class="nav navbar-nav-custom pull-right">

                <!-- User Dropdown -->
                <li class="dropdown">
                    <a href="javascript:void(0)" class="dropdown-toggle" data-toggle="dropdown">
                        <i class="fa fa-sign-in" style="margin-bottom: 13px; margin-top: 13px;"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right">

                        <li>
                            <a href="/logout">
                                <i class="fa fa-sign-out fa fa-inbox fa-fw pull-right"></i>
                                Logout
                            </a>
                        </li>


                    </ul>
                </li>
                <!-- END User Dropdown -->
            </ul>    
        </header>
        <!-- END Header -->

        <!-- Page content -->
        <div id="page-content">

            <!-- Table Styles Block -->
            <div class="block">

                <!-- Table Styles Content -->
                <div class="table-responsive">

                    <!-- Message List -->
                    <div id="user-inbox-list">
                        <!-- Title -->
                        <div class="block-title clearfix">
                            <div class="block-options pull-right">
                                <a href="javascript:void(0)" class="btn btn-effect-ripple btn-default" style="overflow: hidden; position: relative;"><i class="fa fa-arrow-left"></i> Prev</a>
                                <a href="javascript:void(0)" class="btn btn-effect-ripple btn-default" style="overflow: hidden; position: relative;">Next <i class="fa fa-arrow-right"></i></a>
                            </div>
                            <div class="block-options pull-left">

                                <a href="javascript:void(0)" class="btn btn-effect-ripple btn-warning" title="" style="overflow: hidden; position: relative;" data-original-title="Mark as Read"><i class="fa fa-star"></i></a>

                                <a href="javascript:void(0)" class="btn btn-effect-ripple btn-danger" title="" style="overflow: hidden; position: relative;" data-original-title="Delete Selected"><i class="fa fa-times"></i></a>
                            </div>
                        </div>
                        <!-- END Title -->

                        <!-- Messages -->
                        <div class="block-content-full" id="admin-inbox">
                            <table class="table table-borderless table-striped table-vcenter remove-margin">
                                <tbody>
                                    <% _(notifications).each(function(notification){
                                        if(notification.get("code")==1){
                                            var image = notification.get("image");
                                            var user = notification.get("user");
                                            var title = "Ads Approval";
                                        }else if(notification.get("code")==2){
                                            var title = "Pending User Cashout";
                                        }else if(notification.get("code")==3){
                                            var title = "User/Merchant Feedback";
                                        }
                                        var message = notification.get("message");
                                    %>
                                    <!-- Use the first row as a prototype for your column widths -->
                                    <tr>
                                        <td class="td-label td-label-muted text-center" style="width: 5%;">
                                            <label class="csscheckbox csscheckbox-primary"><input type="checkbox"><span></span></label>
                                        </td>

                                        <td>
                                            <a href="javascript:void(0)" class="text-dark" title="
                                                <%=message%>">
                                                <strong>
                                                    <%=title%>
                                                    <br>
                                                </strong>
                                            </a>
                                            <span class="text-muted"><%=message.substring(0,50)%></span>
                                        </td>
                                        <td class="hidden-xs text-center" style="width: 30px;">
                                            <i class="fa fa-paperclip fa-2x text-muted"></i>
                                        </td>
                                        <td class="hidden-xs text-right text-muted" style="width: 120px;"><em>just now</em></td>
                                    </tr>
                                    <% 
                                    });
                                %>
                                </tbody>
                            </table>
                        </div>
                        <!-- END Messages -->
                    </div>
                    <!-- END Message List -->

                </div>
                <!-- END Table Styles Content -->
            </div>
            <!-- END Table Styles Block -->
        </div>
        <!-- END Page Content -->
    </div>
    <!-- END Main Container -->
</div>
<!-- END Page Container -->
</div>
<!-- END Page Wrapper -->