						<a class="btn btn-effect-ripple btn-primary" data-category="feedbacks" style="overflow: hidden; position: relative;"><span class="btn-ripple animate" style="height: 64px; width: 64px; top: -24px; left: -1px;"></span>Feedbacks</a>
					</div>
				</div>
				<!-- Table Styles Block -->
				<div class="block">
					<div>
						<!-- Table Styles Content -->
						<div class="table-responsive">

							<!-- Message List -->
							<div id="admin-inbox-message-list">
								<!-- Title -->
								<div class="block-title clearfix">
									<!-- <div class="block-options pull-right">
										<a class="btn btn-effect-ripple btn-default" style="overflow: hidden; position: relative;"><i class="fa fa-arrow-left"></i> Prev</a>
										<a class="btn btn-effect-ripple btn-default" style="overflow: hidden; position: relative;">Next <i class="fa fa-arrow-right"></i></a>
									</div> -->
									<div class="block-options pull-left">

										<a class="btn btn-effect-ripple btn-warning" title="" style="overflow: hidden; position: relative;" data-original-title="Mark as Read"><i class="fa fa-star"></i></a>

										<a class="btn btn-effect-ripple btn-danger" title="" style="overflow: hidden; position: relative;" data-original-title="Delete Selected"><i class="fa fa-times"></i></a>
									</div>
								</div>
								<!-- END Title -->

								<!-- Messages -->
								<div class="block-content-full" id="admin-inbox">
									<table class="table table-borderless table-hover table-vcenter remove-margin">
										<tbody>
											<% _(notifications).each(function(notification){
												if(notification.get("user")!=undefined){

													var title = ""; 
													var timestamp = "";
													var message = "";
													var user = "";
													var imageUrl = "";
													var imageId = "";
													var code = notification.get("code");

													if(code==1){
														if (notification.get("image")!=null){
															imageUrl = notification.get("image").get('sizeNormal').url();
															imageId = notification.get("image").id;
														}
														user = notification.get("user").get("username");
														title = "Ads Approval";
													}else if(code==2){
														title = "Pending User Cashout";
														user = notification.get("user").get("username");

													}else if(code==3){
														title = "User/Merchant Feedback";
														user = notification.get("user").get("username");
													}
													message = notification.get("message");
													timeTemp = notification.createdAt;

													var buffer = new Date(timeTemp.getTime()+timeTemp.getTimezoneOffset()*60*1000);
													var offset = timeTemp.getTimezoneOffset() / 60;
													var hours = timeTemp.getHours();
													buffer.setHours(hours - offset);
													timeTemp = buffer;  

													var dd = timeTemp.getDate();
											var mm = timeTemp.getMonth()+1; //January is 0!
											var yyyy = timeTemp.getFullYear();
											var hours = timeTemp.getHours();
											var minutes = timeTemp.getMinutes();
											var seconds = timeTemp.getSeconds();

											if(dd<10){
												dd='0'+dd;
											} 
											if(mm<10){
												mm='0'+mm;
											} 
											if(hours<10){
												hours='0'+hours;
											} 
											if(minutes<10){
												minutes='0'+minutes;
											} 
											if(seconds<10){
												seconds='0'+seconds;
											} 
											var timestamp = dd+'/'+mm+'/'+yyyy+" "+hours+":"+minutes+":"+seconds;


											%>
											<!-- Use the first row as a prototype for your column widths -->
											<tr>
												<td class="td-label td-label-muted text-center" style="width: 5%;">
													<label class="csscheckbox csscheckbox-primary"><input type="checkbox"><span></span></label>
												</td>

												<td>
													<h4>
														<a class="text-dark" title="
														<%=title+'|'+timestamp+'|'+user+'|'+message+'|'+imageUrl+'|'+imageId%>">
														<strong>
															<%=title%>
															<br>
														</strong>
													</a>
												</h4>
												<span class="text-muted"><%=message%></span>
											</td>
											<td class="hidden-xs text-center" style="width: 30px;">
												<i class="fa fa-paperclip fa-2x text-muted"></i>
											</td>
											<td class="hidden-xs text-right text-muted" style="width: 120px;"><em><%=timestamp%></em></td>
										</tr>
										<% 
									}});
									%>
								</tbody>
							</table>
						</div>
						<!-- END Messages -->
