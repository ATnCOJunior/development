            <!-- Wrapper for scrolling functionality -->
            <div id="sidebar-scroll">
                <!-- Sidebar Content -->
                <div class="sidebar-content">
                    <!-- Sidebar Navigation -->
                    <ul class="sidebar-nav">

                        <li class="sidebar-separator">
                            <i class="fa fa-ellipsis-h"></i>
                        </li>

                        <li>
                            <a href="" class="active">
                                <i class="gi gi-eye_open sidebar-nav-icon"></i>
                                <span class="sidebar-nav-mini-hide">Ads Approval</span>
                            </a>
                        </li>

                        <li>
                            <a href="#" class="sidebar-nav-menu">
                                <i class="fa fa-chevron-left sidebar-nav-indicator sidebar-nav-mini-hide"></i>
                                <i class="gi gi-group sidebar-nav-icon"></i>
                                <span class="sidebar-nav-mini-hide">Account Management</span>
                            </a>
                            <ul>
                                <li>
                                    <a href="">Customer</a>
                                </li>
                                <li>
                                    <a href="">Merchant</a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="#" class="sidebar-nav-menu">
                                <i class="fa fa-chevron-left sidebar-nav-indicator sidebar-nav-mini-hide"></i>
                                <i class="fa fa-dollar sidebar-nav-icon"></i>
                                <span class="sidebar-nav-mini-hide">Transactions</span>
                            </a>
                            <ul>
                                <li>
                                    <a href="transaction-admin_merchant">Merchant Payment</a>
                                </li>
                                <li>
                                    <a href="transaction-admin_user">Customer Cashout</a>
                                </li>
                            </ul>
                        </li>

                        <li>
                            <a href="">
                                <i class="fa fa-inbox sidebar-nav-icon"></i>
                                <span class="sidebar-nav-mini-hide">Inbox</span>
                            </a>
                        </li>
                    </ul>
                    <!-- END Sidebar Navigation -->
                </div>
                <!-- END Sidebar Content -->
            </div>
            <!-- END Wrapper for scrolling functionality -->

            <!-- Sidebar Extra Info -->
            <div id="sidebar-extra-info" class="sidebar-content sidebar-nav-mini-hide">
                <div class="text-center">
                    <small>Crafted with <i class="fa fa-heart text-danger"></i> by AT+CO.JR. </small>
                </div>
            </div>
            <!-- END Sidebar Extra Info -->
        </div>
        <!-- END Main Sidebar -->

        <!-- Main Container -->
        <div id="main-container">

            <header class="navbar navbar-inverse navbar-fixed-top">
                <!-- Left Header Navigation -->
                <ul class="nav navbar-nav-custom">
                    <!-- Main Sidebar Toggle Button -->

                    <li>
                        <a href="javascript:void(0)" onclick="App.sidebar('toggle-sidebar');">
                            <i class="fa fa-ellipsis-v fa-fw animation-fadeInRight" id="sidebar-toggle-mini"></i>
                            <i class="fa fa-bars fa-fw animation-fadeInRight" id="sidebar-toggle-full"></i>
                        </a>
                    </li>
                    <!-- END Main Sidebar Toggle Button -->

                    <!-- Header Link -->
                    <li class="hidden-xs animation-fadeInQuick">
                        <a><strong>Admin Console</strong></a>
                    </li>
                    <!-- END Header Link -->
                </ul>
                <!-- END Left Header Navigation -->

                <!-- Right Header Navigation -->
                <ul class="nav navbar-nav-custom pull-right">
                    <!-- Search Form -->
                            <!-- <li>
                                <form action="page_ready_search_results.html" method="post" class="navbar-form-custom" role="search">
                                    <input type="text" id="top-search" name="top-search" class="form-control" placeholder="Search..">
                                </form>
                            </li> -->
                            <!-- User Dropdown -->
                            <li class="dropdown">
                                <a href="javascript:void(0)" class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="fa fa-sign-in"></i>
                                </a>
                                <ul class="dropdown-menu dropdown-menu-right">

                                    <li>
                                        <a href="/logout">
                                            <i class="gi gi-log_out fa fa-inbox fa-fw pull-right"></i>
                                            Logout
                                        </a>
                                    </li>
                                    
                                    
                                </ul>
                            </li>
                            <!-- END User Dropdown -->
                        </ul>    
                    </header>
                    <!-- END Header -->

                    <!-- Page content -->
                    <div id="page-content">

                        <!-- Gallery Header -->
                        <div class="content-header">
                            <!-- Gallery Filter Links -->
                            <!-- Custom Gallery functionality is initialized in js/pages/compGallery.js -->
                            <!-- Add the category value you want each link in .gallery-filter to filter out in its data-category attribute. Add the value 'all' to show all items -->
                            <div class="header-section text-center">
                                <div class="btn-group gallery-filter">
                                    <a href="javascript:void(0)" class="btn btn-effect-ripple btn-primary active" data-category="all" style="overflow: hidden; position: relative;"><span class="btn-ripple animate" style="height: 43px; width: 43px; top: -9.5px; left: 13.5px;"></span>All</a>
                                    <a href="javascript:void(0)" class="btn btn-effect-ripple btn-primary" data-category="current" style="overflow: hidden; position: relative;"><span class="btn-ripple animate" style="height: 64px; width: 64px; top: -24px; left: -1px;"></span>Current</a>
                                    <a href="javascript:void(0)" class="btn btn-effect-ripple btn-primary" data-category="pending" style="overflow: hidden; position: relative;"><span class="btn-ripple animate" style="height: 57px; width: 57px; top: -0.5px; left: 12.5px;"></span>Pending</a>
                                    <a href="javascript:void(0)" class="btn btn-effect-ripple btn-primary" data-category="rejected" style="overflow: hidden; position: relative;"><span class="btn-ripple animate" style="height: 70px; width: 70px; top: -9px; left: -18px;"></span>Rejected</a>
                                    <a href="javascript:void(0)" class="btn btn-effect-ripple btn-primary" data-category="expired" style="overflow: hidden; position: relative;"><span class="btn-ripple animate" style="height: 70px; width: 70px; top: -9px; left: -18px;"></span>Expired</a>
                                </div>
                            </div>
                            <!-- END Gallery Filter Links -->
                        </div>
                        <!-- END Gallery Header -->


                        <!-- Gallery Items -->
                        <!-- Lightbox functionality is based on Magnific Popup plugin (initialized in js/app.js -> uiInit()) -->
                        <!-- Add the category value for each item in its data-category attribute (for the filter functionality to work) -->

                        <!-- Okay, this part is for retrieval of all images. and all the images, will have a data-category tag in the class: gallery-image-container-->
                        <div class="row gallery">
                            <% _(images).each(function(image){
                                var imageMetadata = image.get('imageMetadata');
                                if (imageMetadata.get("approval")==0) {%>
                                <div class="col-sm-3">
                                    <div class="gallery-image-container animation-fadeInQuick2" data-category="<%=imageMetadata.get("category")%>"> 
                                        <!-- <div class="gallery-image-container animation-fadeInQuick2" data-category="cafe">  -->
                                        <img src="<%= image.get('sizeNormal').url() %>"
                                        alt="<%= imageMetadata.get("desc")+"|||"
                                        +imageMetadata.get("expiry")+"|||"
                                        +imageMetadata.get("location") %>" 
                                        >
                                        <a href="<%= image.get('sizeNormal').url() %>" class="gallery-image-options" data-toggle="lightbox-image" 
                                            title=" 
                                            <%= imageMetadata.get("desc") 
                                            + "<br>" 
                                            + imageMetadata.get("expiry") 
                                            + "<br>" 
                                            + imageMetadata.get("location")%>">

                                            <h2 class="text-light">
                                                <%=imageMetadata.get("title")%>
                                            </h2>
                                            <i class="fa fa-search-plus fa-3x text-light"></i>

                                        </a>
                                     <!-- <a href="/i/<%=image.id%>" class="gallery-image-options" data-toggle="lightbox-image" title="Image Info">
                                        <h2 class="text-light"><strong><%=imageMetadata.get("title")%></strong></h2>
                                        <i class="fa fa-search-plus fa-3x text-light"></i>
                                    </a> -->
                                </div>
                                <!-- bookmark like share button -->
                                <div class="modal-footer">

                                    <button onClick="location.href='/i/<%=image.id%>/approve'" type="button" class="btn btn-effect-ripple btn-success">
                                        <i class="fa fa-check-circle"></i> Approve
                                    </button>
                                    <button onClick="location.href='/i/<%=image.id%>/reject'" type="button" class="btn btn-effect-ripple btn-info" data-dismiss="modal">
                                        <i class="fa fa-times-circle"></i></i> Reject
                                    </button>
                                    
                                </div>
                                <!-- END bookmark like share button -->
                            </div>
                            <%}});%>
                        </div>
                        <!-- END Gallery Items -->
                    </div>
                    <!-- END Page Content -->
                </div>
                <!-- END Main Container -->
            </div>
            <!-- END Page Container -->
        </div>
        <!-- END Page Wrapper -->