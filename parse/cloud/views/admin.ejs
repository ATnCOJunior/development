<div class="w-row bod">
  <!-- left side navigation codes -->
  <div class="w-col w-col-1 nav"><a class="w-inline-block nav-img text" href="#ads"><h2 class="admin" data-ix="hover">ads</h2></a><a class="w-inline-block nav-img text" href="#reports"><h2 class="admin" data-ix="hover">reports</h2></a>
    <a class="w-inline-block nav-img text" href="#redeem">
      <h2 class="admin" data-ix="hover">redemption</h2>
    </a><a class="w-inline-block nav-img text" href="#pay"><h2 class="admin" data-ix="hover">payment</h2></a>
    <a class="w-inline-block nav-img" href="#ads" data-ix="over">
      <img class="centertop" src="images/Chef-Cap-256.png" >
    </a><a class="w-inline-block nav-img text" href="/logout"><h2 class="admin" data-ix="over">logout</h2></a>
  </div>
  <!-- end of left side navigation codes -->

  <!-- start of main body codes -->
  <div class="w-col w-col-10 main-admin">
    <div class="w-clearfix head pendads">
      <h1 class="adminh1" id="ads">PENDING ADS</h1>
      <div class="w-clearfix image" data-ix="click-show-pending">
        <% _(images).each(function(image) { 
          if (image.get("approval")==0) { %>
        <img class="imgadmin" src="<%= image.get('sizeOriginal').url() %>" width="372" alt="<%= image.get('sizeOriginal').url() %>">
        <div class="like">
          <a href="/approve"><h4 class="count">approve</h4></a>
        </div>
        <div class="share">
          <a href="/reject"><h4 class="count">reject</h4></a>
        </div>
        <% }}); %>
      </div>
      
      <div class="pendinginfo" data-ix="click-hide-info">
        <div class="w-row inforow">
          <div class="w-col w-col-6 info">
            <img class="nav-img" src="images/10533753_786254331394684_4262681625243092472_n.jpg" width="372" alt="53e49766b867b3402d273e63_10533753_786254331394684_4262681625243092472_n.jpg">
          </div>
          <div class="w-col w-col-6 info">
            <h1>Pending Promotion</h1>
            <h4>Promotion Details<br>DING TAI FUNG<br>1-for-1 deals<br>monday to friday<br>2pm - 5pm</h4>
            <h5>Expiry Date: DDMMYYYY<br><br>Category: CAFE</h5>
            <div class="w-container infoactive">
              <a class="w-inline-block infoicon" href="#">
                <img class="infoimg" src="images/Submit-01-256.png" alt="53e4d6ddb867b3402d274544_Submit-01-256.png">
                <h2>approve</h2>
              </a>
              <a class="w-inline-block infoicon" href="#">
                <img class="infoimg" src="images/Delete-256.png" alt="53e4d76aa4bf49cb45b75595_Delete-256.png">
                <h2>reject</h2>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- start of report codes -->
    <div class="head">
      <h1 class="adminh1" id="reports">REPORTS</h1>
      <div class="report"></div>
    </div>
    <!-- end of report codes -->


    <!-- start of merchant codes -->
    <div class="head">
      <h1 class="adminh1" id="merchants">MERCHANTS</h1>
      <div class="merchant">
        <div class="w-container data"></div>
      </div>
    </div>
    <!-- end of merchant codes -->


    <!-- start of user codes -->
    <div class="head">
      <h1 class="adminh1" id="users">USERS</h1>
      <div class="users">
        <div class="w-container data"></div>
      </div>
    </div>
    <!-- end of user codes -->



    <!-- start of popularity codes -->
    <div class="head">
      <h1 class="adminh1" id="pops">POPULARITY</h1>
      <div class="pops">
        <div class="w-container data"></div>
      </div>
    </div>
    <!-- end of popularity codes -->



    <!-- start of redemption codes -->
    <div class="head">
      <h1 class="adminh1" id="redeem">REDEMPTION</h1>
      <div class="redem">
        <div class="w-container data"></div>
      </div>
    </div>
    <!-- end of redemption codes -->


    <!-- start of payment codes -->
    <div class="head">
      <h1 class="adminh1" id="pay">PAYMENT</h1>
      <div class="pay">
        <div class="w-container data"></div>
      </div>
    </div>
    <!-- end of payment codes -->

  </div>
  <!-- end of main body codes -->

  <!-- start of right navigation codes -->
  <div class="w-col w-col-1 navright"><a class="w-inline-block nav-img text" href="#merchants"><h2 class="admin" data-ix="hover">merchants</h2></a><a class="w-inline-block nav-img text" href="#users"><h2 class="admin" data-ix="hover">users</h2></a><a class="w-inline-block nav-img text"
    href="#pops"><h2 class="admin" data-ix="hover">popularity</h2></a>
  </div>
  <!-- end of right navigtion codes -->
</div>