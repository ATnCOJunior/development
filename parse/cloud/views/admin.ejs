<!-- start of main body codes -->
<div class="w-col w-col-10 main-admin">

  <div class="middletop">
    <a class="w-inline-block link" >
      <img class="centertop" src="images/Chef-Cap-256.png" >
    </a>
  </div>

  <div class="w-clearfix head pendads">
    <h1 class="adminh1" id="ads">PENDING ADS</h1>

    <% _(images).each(function(image) { 
      if (image.get("approval")=="0") {
        %>
        <div class="w-clearfix image" data-ix="click-show-pending">
          <img class="imgadmin" src="<%= image.get('sizeNormal').url() %>" width="372">
          <div class="approve" metadataId="<%= image.get("imageMetadata").id %>">
            <h4 class="count">approve</h4>
          </div>
          <div class="reject" metadataId="<%= image.get("imageMetadata").id %>">
            <h4 class="count">reject</h4>
          </div>
        </div>
        <% 
      }
    }); %>

    <div class="pendinginfo" data-ix="click-hide-info">
      <div class="w-row inforow">
        <div class="w-col w-col-6 info">
          <img id="popup-image" class="nav-img" src="images/10533753_786254331394684_4262681625243092472_n.jpg" width="372" >
        </div>
        <div class="w-col w-col-6 info">
          <h1>Pending Promotion</h1>
          <h4>Promotion Details<br>DING TAI FUNG<br>1-for-1 deals<br>monday to friday<br>2pm - 5pm</h4>
          <h5>Expiry Date: DDMMYYYY<br>
            <br>Category: CAFE</h5>
            <div class="w-container infoactive">
              <a class="w-inline-block infoicon" href="#">
                <img id="approve-button" class="infoimg" src="images/Submit-01-256.png">
                <h2>approve</h2>
              </a>
              <a class="w-inline-block infoicon" href="#">
                <img id="reject-button" class="infoimg" src="images/Delete-256.png">
                <h2>reject</h2>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="head">
      <h1 class="adminh1" id="reports">REPORTS</h1>
      <div class="report">
      </div>
    </div>
    <div class="head">
      <h1 class="adminh1" id="merchants">MERCHANTS</h1>
      <div class="merchant">
        <div class="w-container data">
        </div>
      </div>
    </div>
    <div class="head">
      <h1 class="adminh1" id="users">USERS</h1>
      <div class="users">
        <div class="w-container data">
        </div>
      </div>
    </div>
    <div class="head">
      <h1 class="adminh1" id="pops">POPULARITY</h1>
      <div class="pops">
        <div class="w-container data">
        </div>
      </div>
    </div>
    <div class="head">
      <h1 class="adminh1" id="redeem">REDEMPTION</h1>
      <div class="redem">
        <div class="w-container data">
        </div>
      </div>
    </div>
    <div class="head">
      <h1 class="adminh1" id="pay">PAYMENT</h1>
      <div class="pay">
        <div class="w-container data">
        </div>
      </div>
    </div>
  </div>  
</div>
<!-- end of main body codes -->



