            <label for="company-name">Company Name</label>
            <input class="w-input field" id="company-name" type="text" placeholder="Enter your company name" name="company" data-name="company name" required="required" autofocus="autofocus">
            <label for="email">Email</label>
            <input class="w-input" id="password" type="email" placeholder="Enter your email" name="password" data-name="Email 2" required="required" autofocus="autofocus">
            <label class="label" for="email-2">Password</label>
            <input class="w-input" id="email" type="password" placeholder="Enter your password" name="email" data-name="email" required="required" autofocus="autofocus">
