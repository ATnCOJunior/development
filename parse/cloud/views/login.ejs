            <input class="w-input" id="site" type="text" placeholder="Enter your website URL" name="website" data-name="site" required="required" autofocus="autofocus">
            <label for="social-media-Site">Social Media Site</label>
            <input class="w-input" id="social-media-Site" type="text" placeholder="Enter your social media sites redirections" name="social" data-name="social media Site" required="required" autofocus="autofocus">
            <label for="description">Company Description</label>
            <input class="w-input" id="description" type="text" placeholder="Enter your company category" name="desc" data-name="description" required="required" autofocus="autofocus"></input>
            <input class="w-button go" type="submit" value="register" data-wait="Please wait...">
          </form>

