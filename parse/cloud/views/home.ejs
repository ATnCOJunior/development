                                <div class="col-sm-6 col-sm-offset-3">
                                    <div class="header-section">
                                        <form action="page_ready_search_results.html" method="post" onsubmit="return false;">
                                            <div class="form-group">
                                                <select id="search-categories" name="search-categories" class="select-chosen" data-placeholder="Choose categories.." style="width: 250px;" >
                                                    
                                                    <option value="current">Cafe</option>
                                                    <option value="pending">Bars</option>
                                                    <option value="pending">Dining</option>
                                                    
                                                </select>
