                                        <div class="col-sm-4">
                                            <div class="gallery-image-container animation-fadeInQuick">
                                                <% _(images).each(function(image) { 
      var imageMetadata = image.get('imageMetadata');
      if (imageMetadata.get("approval")=="0") {%>
        <img class="adsimage" src="<%= image.get('sizeNormal').url() %>" width="372" 

          alt="<%= imageMetadata.get("desc")+"|||"
            +imageMetadata.get("expiry")+"|||"
            +imageMetadata.get("location") %>" 

          data-ix="click-show">

       
        </div>
      <% }}); %>
                                            </div>
                                        </div>
                                        
                                    </div>
                                    
                                </div>
