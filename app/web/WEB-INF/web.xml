<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>MerchantLoginServlet</servlet-name>
        <servlet-class>MerchantLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bootstrap</servlet-name>
        <servlet-class>Utility.Bootstrap</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MerchantLoginServlet</servlet-name>
        <url-pattern>/merchant_login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MerchantRegisterServlet</servlet-name>
        <servlet-class>MerchantRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MerchantRegisterServlet</servlet-name>
        <url-pattern>/merchant_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bootstrap</servlet-name>
        <url-pattern>/uploadFile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
